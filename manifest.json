{
    "manifest_version": 3,
    "name": "LinkedIn Email Extractor",
    "version": "1.0",
    "description": "Extract emails from LinkedIn profiles",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://www.linkedin.com/in/*"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  